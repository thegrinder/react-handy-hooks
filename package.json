{
  "name": "react-handy-hooks",
  "version": "1.6.0",
  "description": "Set of useful react hooks",
  "keywords": [
    "react",
    "hooks",
    "form",
    "accordion",
    "accessibility"
  ],
  "homepage": "https://thegrinder.github.io/react-handy-hooks",
  "repository": {
    "type": "git",
    "url": "https://github.com/thegrinder/react-handy-hooks.git"
  },
  "main": "./dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "lint": "eslint './src/**/*.js' --fix",
    "production": "webpack",
    "prepublishOnly": "npm run production",
    "test": "jest",
    "docz:build": "docz build",
    "docz:dev": "docz dev",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "author": "Pawel Szczepkowski",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@semantic-release/git": "^7.0.16",
    "@semantic-release/npm": "^5.1.13",
    "@testing-library/react-hooks": "^1.1.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "docz": "^1.2.0",
    "docz-theme-default": "^1.2.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.6.0",
    "husky": "^3.0.2",
    "jest": "^24.8.0",
    "jest-dom": "^4.0.0",
    "lint-staged": "^9.2.1",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "semantic-release": "^15.13.19",
    "webpack": "4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "peerDependencies": {
    "react": "^16.8.6"
  },
  "dependencies": {
    "fast-deep-equal": "^2.0.1"
  }
}
