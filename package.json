{
  "name": "react-handy-hooks",
  "version": "1.5.0",
  "description": "Set of useful react hooks",
  "keywords": [
    "react",
    "hooks",
    "form",
    "accordion",
    "accessibility"
  ],
  "homepage": "https://thegrinder.github.io/react-handy-hooks",
  "repository": {
    "type": "git",
    "url": "https://github.com/thegrinder/react-handy-hooks.git"
  },
  "main": "./dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "lint": "eslint './src/**/*.js' --fix",
    "production": "webpack",
    "prepublishOnly": "npm run production",
    "test": "jest",
    "docz:build": "docz build",
    "docz:dev": "docz dev",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "author": "Pawel Szczepkowski",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/npm": "^5.1.9",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "docz": "^1.2.0",
    "docz-theme-default": "^1.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "jest-dom": "^3.4.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hooks-testing-library": "^0.5.0",
    "react-test-renderer": "^16.8.6",
    "semantic-release": "^15.13.14",
    "webpack": "4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.5.1"
  },
  "peerDependencies": {
    "react": "^16.8.6"
  },
  "dependencies": {
    "fast-deep-equal": "^2.0.1"
  }
}
